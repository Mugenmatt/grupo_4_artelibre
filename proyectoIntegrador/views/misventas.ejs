<!DOCTYPE html>
<html lang="en">

<%- include('partials/head', {title: 'Mis ventas', css: 'myart.css', labs:""}) %>

<body>
    <div class="hero">
        <%- include('partials/header') %>
    </div>

<main class="todo container">

    <a class="linkprofile" href="/users/profile"><i class="material-icons">navigate_before</i>Volver a mi perfil</a>

    <table class="highlight">
        <thead>
            <tr>

                <th>Obra</th>
                <th>Titulo</th>
                <th>Fecha</th>
                <th>Cliente</th>
                <th>Total</th>              
            </tr>
        </thead>

        <tbody>
            <% for(let item of items) { %>
                <tr>
                    <td>
                        <div class="card imagen-producto"
                            style="background-image:url(/images/products/<%= item.product.imageFile %>">
                        </div>
                    </td>
                    <td><%= item.product.name %> </td>
                    <td > <%= new Date(item.updatedAt).toLocaleDateString()%> </td>
                    <td><%=item.user.name %></td>
                    <td>$ <%= item.price %> </td>
                </tr>
            <% } %>

        </tbody>
    </table>


   
    </div>



</main>

    <%- include('partials/footer.ejs') %>

    <script src="https://code.jquery.com/jquery-3.5.1.min.js"
        integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
    <script src="/js/header-function.js"></script>
    <script src="/js/myart-function.js"></script>

</body>

</html>